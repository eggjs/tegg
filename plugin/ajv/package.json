{
  "name": "@eggjs/tegg-ajv-plugin",
  "eggPlugin": {
    "name": "teggAjv",
    "strict": false,
    "dependencies": [
      "tegg"
    ]
  },
  "eggModule": {
    "name": "teggAjv"
  },
  "version": "4.0.0-beta.4",
  "description": "ajv plugin for egg and tegg",
  "keywords": [
    "egg",
    "plugin",
    "typescript",
    "module",
    "tegg",
    "ajv"
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "default": "./src/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "lib/**/*.js",
    "lib/**/*.d.ts",
    "typings/**/*.d.ts"
  ],
  "scripts": {
    "clean": "tsc -b --clean",
    "tsc": "npm run clean && tsc -p ./tsconfig.json",
    "tsc:pub": "npm run tsc",
    "prepublishOnly": "npm run tsc"
  },
  "homepage": "https://github.com/eggjs/tegg",
  "bugs": {
    "url": "https://github.com/eggjs/tegg/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:eggjs/tegg.git",
    "directory": "plugin/ajv"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@eggjs/ajv-formats": "^3.0.1",
    "@eggjs/ajv-keywords": "^5.1.0",
    "@eggjs/tegg": "^4.0.0-beta.4",
    "@sinclair/typebox": "^0.32.20",
    "ajv": "^8.17.1"
  },
  "devDependencies": {
    "@eggjs/mock": "6",
    "@eggjs/tegg-config": "^4.0.0-beta.4",
    "@eggjs/tegg-controller-plugin": "^4.0.0-beta.4",
    "@eggjs/tegg-plugin": "^4.0.0-beta.4",
    "@types/node": "22",
    "egg": "4",
    "ts-node": "10",
    "typescript": "5"
  },
  "publishConfig": {
    "access": "public"
  }
}
