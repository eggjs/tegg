{
  "name": "@eggjs/tegg-mcp-client",
  "eggPlugin": {
    "name": "teggMcpClient",
    "dependencies": [
      "tegg"
    ]
  },
  "eggModule": {
    "name": "teggMcpClient"
  },
  "version": "4.0.0-beta.11",
  "description": "mcp client for egg",
  "keywords": [
    "egg",
    "plugin",
    "typescript",
    "module",
    "tegg",
    "mcp"
  ],
  "files": [
    "dist"
  ],
  "type": "module",
  "exports": {
    ".": "./src/index.ts",
    "./package.json": "./package.json"
