    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "homepage": "https://github.com/eggjs/tegg/tree/next/plugin/mcp-client",
  "bugs": {
    "url": "https://github.com/eggjs/tegg/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:eggjs/tegg.git",
    "directory": "plugin/mcp-client"
  },
  "engines": {
    "node": ">=22.18.0"
  },
  "dependencies": {
    "@eggjs/egg-module-common": "workspace:*",
    "@eggjs/mcp-client": "workspace:*",
    "@eggjs/tegg": "workspace:*",
    "@eggjs/tegg-common-util": "workspace:*",
    "@eggjs/tegg-runtime": "workspace:*",
    "@modelcontextprotocol/sdk": "^1.23.0",
    "sdk-base": "catalog:",
    "urllib": "catalog:"
  },
  "peerDependencies": {
    "egg": "catalog:",
    "@eggjs/tegg-plugin": "workspace:*"
  },
  "devDependencies": {
    "@eggjs/mock": "catalog:",
    "@eggjs/tegg-config": "workspace:*",
    "@eggjs/tegg-plugin": "workspace:*",
    "@types/node": "catalog:",
    "egg": "catalog:",
    "typescript": "catalog:",
    "tsdown": "catalog:",
    "unplugin-unused": "catalog:"
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": "./dist/index.js",
      "./package.json": "./package.json"
    }
