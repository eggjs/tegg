{
  "name": "@eggjs/tegg-dal-plugin",
  "eggPlugin": {
    "name": "teggDal",
    "strict": false,
    "dependencies": [
      "tegg"
    ]
  },
  "eggModule": {
    "name": "teggDal"
  },
  "version": "4.0.0-beta.11",
  "description": "dal plugin for egg",
  "keywords": [
    "egg",
    "plugin",
    "typescript",
    "module",
    "tegg",
    "dal"
  ],
  "files": [
    "dist"
  ],
  "type": "module",
  "exports": {
    ".": "./src/index.ts",
    "./app": "./src/app.ts",
    "./lib/DalModuleLoadUnitHook": "./src/lib/DalModuleLoadUnitHook.ts",
    "./lib/DalTableEggPrototypeHook": "./src/lib/DalTableEggPrototypeHook.ts",
    "./lib/DataSource": "./src/lib/DataSource.ts",
    "./lib/MysqlDataSourceManager": "./src/lib/MysqlDataSourceManager.ts",
    "./lib/SqlMapManager": "./src/lib/SqlMapManager.ts",
    "./lib/TableModelManager": "./src/lib/TableModelManager.ts",
    "./lib/TransactionalAOP": "./src/lib/TransactionalAOP.ts",
    "./lib/TransactionPrototypeHook": "./src/lib/TransactionPrototypeHook.ts",
    "./types": "./src/types.ts",
    "./package.json": "./package.json"
  },
  "scripts": {
    "test:mocha": "egg-bin test",
    "cov:mocha": "egg-bin cov",
    "clean": "rimraf dist *.tsbuildinfo",
    "build": "tsdown && npm run clean && tsc -p tsconfig.build.json",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "homepage": "https://github.com/eggjs/tegg/tree/next/plugin/dal",
  "bugs": {
    "url": "https://github.com/eggjs/tegg/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:eggjs/tegg.git",
    "directory": "plugin/dal"
  },
  "engines": {
    "node": ">=22.18.0"
  },
  "dependencies": {
    "@eggjs/dal-runtime": "workspace:*",
    "@eggjs/tegg": "workspace:*",
    "@eggjs/tegg-metadata": "workspace:*",
    "@eggjs/dal-decorator": "workspace:*"
  },
  "peerDependencies": {
    "egg": "catalog:",
    "@eggjs/tegg-plugin": "workspace:*"
  },
  "devDependencies": {
    "@eggjs/bin": "catalog:",
    "@eggjs/mock": "catalog:",
    "@eggjs/tegg-aop-plugin": "workspace:*",
    "@eggjs/tegg-config": "workspace:*",
    "@eggjs/tegg-plugin": "workspace:*",
    "@eggjs/tracer": "catalog:",
    "@types/node": "catalog:",
    "@types/mocha": "catalog:",
    "egg": "catalog:",
    "globby": "catalog:",
    "mocha": "catalog:",
    "typescript": "catalog:",
    "tsdown": "catalog:",
    "unplugin-unused": "catalog:"
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": "./dist/index.js",
      "./app": "./dist/app.js",
      "./lib/DalModuleLoadUnitHook": "./dist/lib/DalModuleLoadUnitHook.js",
      "./lib/DalTableEggPrototypeHook": "./dist/lib/DalTableEggPrototypeHook.js",
      "./lib/DataSource": "./dist/lib/DataSource.js",
      "./lib/MysqlDataSourceManager": "./dist/lib/MysqlDataSourceManager.js",
      "./lib/SqlMapManager": "./dist/lib/SqlMapManager.js",
      "./lib/TableModelManager": "./dist/lib/TableModelManager.js",
      "./lib/TransactionalAOP": "./dist/lib/TransactionalAOP.js",
      "./lib/TransactionPrototypeHook": "./dist/lib/TransactionPrototypeHook.js",
      "./types": "./dist/types.js",
      "./package.json": "./package.json"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts"
}
