{
  "name": "@eggjs/tegg-controller-plugin",
  "eggPlugin": {
    "name": "teggController",
    "strict": false,
    "dependencies": [
      "tegg"
    ]
  },
  "version": "4.0.0-beta.4",
  "description": "controller decorator for egg",
  "keywords": [
    "egg",
    "plugin",
    "typescript",
    "module",
    "tegg"
  ],
  "files": [
    "app.js",
    "app.d.ts",
    "lib/**/*.js",
    "lib/**/*.d.ts",
    "app/**/*.js",
    "app/**/*.d.ts",
    "index.js",
    "index.d.ts"
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "default": "./index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "test": "egg-bin test",
    "cov": "egg-bin cov",
    "clean": "tsc -b --clean",
    "tsc": "npm run clean && tsc -p ./tsconfig.pub.json",
    "tsc:pub": "npm run tsc",
    "prepublishOnly": "npm run tsc"
  },
  "homepage": "https://github.com/eggjs/tegg",
  "bugs": {
    "url": "https://github.com/eggjs/tegg/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:eggjs/tegg.git",
    "directory": "plugin/controller"
  },
  "engines": {
    "node": ">=22.18.0"
  },
  "dependencies": {
    "@eggjs/core": "beta",
    "@eggjs/egg-module-common": "^4.0.0-beta.4",
    "@eggjs/router": "beta",
    "@eggjs/tegg": "^4.0.0-beta.4",
    "@eggjs/tegg-common-util": "^4.0.0-beta.4",
    "@eggjs/tegg-loader": "^4.0.0-beta.4",
    "@eggjs/tegg-metadata": "^4.0.0-beta.4",
    "@eggjs/tegg-runtime": "^4.0.0-beta.4",
    "@eggjs/tegg-types": "^4.0.0-beta.4",
    "egg-errors": "^2.3.0",
    "globby": "^14.1.0",
    "path-to-regexp": "^1.9.0",
    "sdk-base": "^5.0.1"
  },
  "devDependencies": {
    "@eggjs/bin": "beta",
    "@eggjs/mock": "beta",
    "@eggjs/module-test-util": "^4.0.0-beta.4",
    "@eggjs/tegg-config": "^4.0.0-beta.4",
    "@eggjs/tegg-plugin": "^4.0.0-beta.4",
    "@eggjs/tracer": "beta",
    "@types/mocha": "10",
    "@types/node": "22",
    "egg": "beta",
    "ts-node": "10",
    "typescript": "5"
  },
  "publishConfig": {
    "access": "public"
  }
}
