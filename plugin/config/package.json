{
  "name": "@eggjs/tegg-config",
  "eggPlugin": {
    "name": "teggConfig"
  },
  "version": "4.0.0-beta.4",
  "description": "module config plugin for egg",
  "keywords": [
    "egg",
    "plugin",
    "typescript",
    "module",
    "config",
    "tegg"
  ],
  "files": [
    "dist"
  ],
  "type": "module",
  "exports": {
    ".": "./src/index.ts",
    "./agent": "./src/agent.ts",
    "./app": "./src/app.ts",
    "./config/config.default": "./src/config/config.default.ts",
    "./lib/ModuleScanner": "./src/lib/ModuleScanner.ts",
    "./types": "./src/types.ts",
    "./package.json": "./package.json"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "tsdown",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "homepage": "https://github.com/eggjs/tegg",
  "bugs": {
    "url": "https://github.com/eggjs/tegg/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:eggjs/tegg.git",
    "directory": "plugin/config"
  },
  "engines": {
    "node": ">=22.18.0"
  },
  "dependencies": {
    "@eggjs/tegg-common-util": "^4.0.0-beta.4",
    "@eggjs/utils": "beta"
  },
  "devDependencies": {
    "@eggjs/mock": "beta",
    "@types/node": "22",
    "egg": "beta",
    "typescript": "5",
    "tsdown": "^0.15.6"
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": "./dist/index.js",
      "./agent": "./dist/agent.js",
      "./app": "./dist/app.js",
      "./config/config.default": "./dist/config/config.default.js",
      "./lib/ModuleScanner": "./dist/lib/ModuleScanner.js",
      "./types": "./dist/types.js",
      "./package.json": "./package.json"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts"
}
