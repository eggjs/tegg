{
  "name": "@eggjs/tegg-aop-runtime",
  "version": "4.0.0-beta.3",
  "description": "tegg aop",
  "eggModule": {
    "name": "teggAopRuntime"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "default": "./src/index.js"
    }
  },
  "files": [
    "src/**/*.js",
    "src/**/*.d.ts"
  ],
  "keywords": [
    "egg",
    "typescript",
    "runtime",
    "tegg",
    "aop"
  ],
  "scripts": {
    "clean": "tsc -b --clean",
    "tsc": "npm run clean && tsc -p ./tsconfig.json",
    "tsc:pub": "npm run tsc",
    "prepublishOnly": "npm run tsc"
  },
  "author": "killagu <killa123@126.com>",
  "license": "MIT",
  "homepage": "https://github.com/eggjs/tegg",
  "bugs": {
    "url": "https://github.com/eggjs/tegg/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:eggjs/tegg.git",
    "directory": "core/aop-runtime"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@eggjs/aop-decorator": "^4.0.0-beta.3",
    "@eggjs/core-decorator": "^4.0.0-beta.3",
    "@eggjs/tegg-common-util": "^4.0.0-beta.3",
    "@eggjs/tegg-lifecycle": "^4.0.0-beta.3",
    "@eggjs/tegg-metadata": "^4.0.0-beta.3",
    "@eggjs/tegg-runtime": "^4.0.0-beta.3",
    "@eggjs/tegg-types": "^4.0.0-beta.3",
    "koa-compose": "^4.1.0"
  },
  "devDependencies": {
    "@eggjs/module-test-util": "^4.0.0-beta.3",
    "@eggjs/tegg-loader": "^4.0.0-beta.3",
    "@types/koa-compose": "3",
    "@types/node": "22",
    "ts-node": "10",
    "typescript": "5"
  }
