{
  "name": "@eggjs/mcp-client",
  "version": "4.0.0-beta.11",
  "description": "tegg mcp client",
  "keywords": [
    "egg",
    "typescript",
    "runtime",
    "tegg"
  ],
  "type": "module",
  "exports": {
    ".": "./src/index.ts",
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist *.tsbuildinfo",
    "build": "tsdown && npm run clean && tsc -p tsconfig.build.json",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "author": "akitaSummer <akitasummer@outlook.com>",
  "license": "MIT",
  "homepage": "https://github.com/eggjs/tegg/tree/next/core/mcp-client",
  "bugs": {
    "url": "https://github.com/eggjs/tegg/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:eggjs/tegg.git",
    "directory": "core/mcp-client"
  },
  "engines": {
    "node": ">=22.18.0"
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": "./dist/index.js",
      "./package.json": "./package.json"
    }
  },
  "dependencies": {
    "@eggjs/core-decorator": "workspace:*",
    "@eggjs/tegg-types": "workspace:*",
    "@langchain/mcp-adapters": "^1.0.0",
    "@modelcontextprotocol/sdk": "^1.23.0",
    "sdk-base": "catalog:",
    "urllib": "catalog:"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "typescript": "catalog:",
    "tsdown": "catalog:",
    "unplugin-unused": "catalog:"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts"
}
