{
  "name": "@eggjs/tegg-loader",
  "version": "4.0.0-beta.1",
  "description": "tegg default loader implement",
  "keywords": [
    "egg",
    "typescript",
    "loader",
    "tegg"
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "default": "./src/index.js"
    }
  },
  "files": [
    "src/**/*.js",
    "src/**/*.d.ts"
  ],
  "scripts": {
    "clean": "tsc -b --clean",
    "tsc": "npm run clean && tsc -p ./tsconfig.json",
    "tsc:pub": "npm run tsc",
    "prepublishOnly": "npm run tsc"
  },
  "author": "killagu <killa123@126.com>",
  "license": "MIT",
  "homepage": "https://github.com/eggjs/tegg",
  "bugs": {
    "url": "https://github.com/eggjs/tegg/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:eggjs/tegg.git",
    "directory": "core/loader"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@eggjs/core-decorator": "^4.0.0-beta.1",
    "@eggjs/tegg-common-util": "^4.0.0-beta.1",
    "@eggjs/tegg-types": "^4.0.0-beta.1",
    "globby": "^14.1.0",
    "is-type-of": "^2.2.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@eggjs/tegg-metadata": "^4.0.0-beta.1",
    "@types/node": "22",
    "ts-node": "10",
    "typescript": "5"
  }
}
