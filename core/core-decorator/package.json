  },
  "repository": {
    "type": "git",
    "url": "git@github.com:eggjs/tegg.git",
    "directory": "core/core-decorator"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "@eggjs/tegg-common-util": "^1.2.3",
    "reflect-metadata": "^0.1.13"
  },
  "publishConfig": {
    "access": "public"
