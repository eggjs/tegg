{
  "name": "@eggjs/core-decorator",
  "version": "3.1.0",
  "description": "tegg core decorator",
  "keywords": [
    "egg",
    "typescript",
    "decorator",
    "tegg"
  ],
  "main": "dist/index.js",
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts"
  ],
