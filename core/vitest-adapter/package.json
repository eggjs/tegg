{
  "name": "@eggjs/tegg-vitest-adapter",
  "version": "4.0.0-beta.11",
  "description": "Vitest adapter for tegg context injection",
  "keywords": [
    "egg",
    "tegg",
    "vitest",
    "test",
    "adapter"
  ],
  "type": "module",
  "exports": {
    ".": "./src/index.ts",
    "./runner": "./src/runner.ts",
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist *.tsbuildinfo",
    "build": "tsdown && npm run clean && tsc -p tsconfig.build.json",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build",
    "test": "vitest run"
  },
  "author": "killagu <killa123@126.com>",
  "license": "MIT",
  "homepage": "https://github.com/eggjs/tegg/tree/next/core/vitest-adapter",
  "bugs": {
    "url": "https://github.com/eggjs/tegg/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:eggjs/tegg.git",
    "directory": "core/vitest-adapter"
  },
  "engines": {
    "node": ">=22.18.0"
  },
  "dependencies": {
    "@eggjs/mock": "catalog:"
  },
  "peerDependencies": {
    "vitest": "catalog:"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "typescript": "catalog:",
    "tsdown": "catalog:",
    "unplugin-unused": "catalog:",
    "vitest": "catalog:",
    "egg": "catalog:"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": "./dist/index.js",
      "./runner": "./dist/runner.js",
      "./package.json": "./package.json"
    }
  }
}
