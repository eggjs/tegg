{
  "name": "@eggjs/tegg-types",
  "version": "4.0.0-beta.4",
  "description": "tegg types",
  "keywords": [
    "egg",
    "typescript",
    "tegg",
    "types"
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "default": "./index.js"
    },
    "./aop": {
      "types": "./aop/index.d.ts",
      "default": "./aop/index.js"
    },
    "./common": {
      "types": "./common/index.d.ts",
      "default": "./common/index.js"
    },
    "./controller-decorator": {
      "types": "./controller-decorator/index.d.ts",
      "default": "./controller-decorator/index.js"
    },
    "./core-decorator": {
      "types": "./core-decorator/index.d.ts",
      "default": "./core-decorator/index.js"
    },
    "./dal": {
      "types": "./dal/index.d.ts",
      "default": "./dal/index.js"
    },
    "./lifecycle": {
      "types": "./lifecycle/index.d.ts",
      "default": "./lifecycle/index.js"
    },
    "./metadata": {
      "types": "./metadata/index.d.ts",
      "default": "./metadata/index.js"
    },
    "./runtime": {
      "types": "./runtime/index.d.ts",
      "default": "./runtime/index.js"
    },
    "./dynamic-inject": {
      "types": "./dynamic-inject.d.ts",
      "default": "./dynamic-inject.js"
    },
    "./orm": {
      "types": "./orm.d.ts",
      "default": "./orm.js"
    },
    "./schedule": {
      "types": "./schedule.d.ts",
      "default": "./schedule.js"
    },
    "./transaction": {
      "types": "./transaction.d.ts",
      "default": "./transaction.js"
    }
  },
  "files": [
    "**/*.js",
    "**/*.d.ts"
  ],
  "scripts": {
    "clean": "tsc -b --clean",
    "tsc:pub": "npm run clean && tsc -p ./tsconfig.json",
    "prepublishOnly": "npm run tsc:pub"
  },
  "author": "gxkl <gxkl203@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/eggjs/tegg",
  "bugs": {
    "url": "https://github.com/eggjs/tegg/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:eggjs/tegg.git",
    "directory": "core/types"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/node": "22",
    "ts-node": "10",
    "typescript": "5"
  }
}
