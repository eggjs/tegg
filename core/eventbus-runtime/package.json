{
  "name": "@eggjs/tegg-eventbus-runtime",
  "version": "3.39.0-alpha.1",
  "description": "tegg eventbus runtime",
  "keywords": [
    "egg",
    "typescript",
    "eventbus",
    "eventemitter",
    "tegg"
  ],
  "eggModule": {
    "name": "eventbusRuntime"
  },
  "main": "dist/index.js",
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "esm/**/*.js",
    "esm/**/*.d.ts"
  ],
  "typings": "dist/index.d.ts",
  "scripts": {
    "test": "cross-env NODE_ENV=test NODE_OPTIONS='--no-deprecation' mocha",
    "clean": "tsc -b --clean && npm run clean:esm",
    "clean:esm": "node ../../scripts/clean-esm.js",
    "tsc": "npm run clean && tsc -p ./tsconfig.json",
    "tsc:esm": "npm run clean:esm && node ../../scripts/build-esm.js &&tsc -p ./tsconfig.esm.json",
    "tsc:pub": "npm run clean && tsc -p ./tsconfig.pub.json && npm run tsc:esm",
    "prepublishOnly": "npm run tsc:pub"
  },
  "author": "killagu <killa123@126.com>",
  "license": "MIT",
  "homepage": "https://github.com/eggjs/tegg",
  "bugs": {
    "url": "https://github.com/eggjs/tegg/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:eggjs/tegg.git",
    "directory": "core/eventbus-runtime"
  },
  "dependencies": {
    "@eggjs/core-decorator": "3.39.0-alpha.1",
    "@eggjs/eventbus-decorator": "3.39.0-alpha.1",
    "@eggjs/tegg-common-util": "3.39.0-alpha.1",
    "@eggjs/tegg-runtime": "3.39.0-alpha.1",
    "@eggjs/tegg-types": "3.39.0-alpha.1",
    "await-event": "^2.1.0",
    "await-first": "^1.0.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "devDependencies": {
    "@eggjs/tegg-loader": "3.39.0-alpha.1",
    "@eggjs/tegg-metadata": "3.39.0-alpha.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.2.4",
    "coffee": "^5.4.0",
    "cross-env": "^7.0.3",
    "egg": "^3.9.1",
    "mm": "^3.2.0",
    "mocha": "^10.2.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
