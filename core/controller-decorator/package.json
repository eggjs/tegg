{
  "name": "@eggjs/controller-decorator",
  "version": "4.0.0-beta.4",
  "description": "tegg controller decorator",
  "keywords": [
    "egg",
    "typescript",
    "decorator",
    "controller",
    "tegg"
  ],
  "type": "module",
