{
  "name": "@eggjs/controller-decorator",
  "version": "4.0.0-beta.3",
  "description": "tegg controller decorator",
  "keywords": [
    "egg",
    "typescript",
    "decorator",
    "controller",
    "tegg"
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "default": "./src/index.js"
    }
  },
  "files": [
    "src/**/*.js",
    "src/**/*.d.ts"
  ],
  "scripts": {
    "clean": "tsc -b --clean",
    "tsc": "npm run clean && tsc -p ./tsconfig.json",
    "tsc:pub": "npm run tsc",
    "prepublishOnly": "npm run tsc:pub"
  },
  "author": "killagu <killa123@126.com>",
  "license": "MIT",
  "homepage": "https://github.com/eggjs/tegg",
  "bugs": {
    "url": "https://github.com/eggjs/tegg/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:eggjs/tegg.git",
    "directory": "core/controller-decorator"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@eggjs/aop-decorator": "^4.0.0-beta.3",
    "@eggjs/cookies": "^3.1.0",
    "@eggjs/core-decorator": "^4.0.0-beta.3",
    "@eggjs/tegg-common-util": "^4.0.0-beta.3",
    "@eggjs/tegg-metadata": "^4.0.0-beta.3",
    "@eggjs/tegg-types": "^4.0.0-beta.3",
    "is-type-of": "^2.2.0",
    "path-to-regexp": "^1.9.0",
    "reflect-metadata": "^0.2.2"
  },
  "devDependencies": {
    "@types/node": "22",
    "egg": "4",
    "ts-node": "10",
    "typescript": "5"
  },
  "publishConfig": {
    "access": "public"
  }
}
