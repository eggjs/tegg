  "scripts": {
    "clean": "rimraf dist",
    "build": "tsdown",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "author": "killagu <killa123@126.com>",
  "license": "MIT",
  "homepage": "https://github.com/eggjs/tegg/tree/next/core/controller-decorator",
  "bugs": {
    "url": "https://github.com/eggjs/tegg/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:eggjs/tegg.git",
    "directory": "core/controller-decorator"
  },
  "engines": {
    "node": ">=22.18.0"
  },
  "dependencies": {
    "@eggjs/aop-decorator": "workspace:*",
    "@eggjs/cookies": "beta",
    "@eggjs/core-decorator": "workspace:*",
    "@eggjs/tegg-common-util": "workspace:*",
    "@eggjs/tegg-metadata": "workspace:*",
    "@eggjs/tegg-types": "workspace:*",
    "is-type-of": "catalog:",
    "path-to-regexp": "^1.9.0",
    "reflect-metadata": "catalog:"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "egg": "beta",
    "typescript": "catalog:",
    "tsdown": "catalog:",
    "unplugin-unused": "catalog:"
  },
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": "./dist/index.js",
