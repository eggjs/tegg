{
  "name": "@eggjs/tegg",
  "version": "4.0.0-beta.1",
  "publishConfig": {
    "access": "public"
  },
  "description": "tegg decorator packages",
  "keywords": [
    "egg",
    "typescript",
    "metadata",
    "tegg"
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "default": "./index.js"
    },
    "./ajv": {
      "types": "./ajv.d.ts",
      "default": "./ajv.js"
    },
    "./aop": {
      "types": "./aop.d.ts",
      "default": "./aop.js"
    },
    "./dal": {
      "types": "./dal.d.ts",
      "default": "./dal.js"
    },
    "./helper": {
      "types": "./helper.d.ts",
      "default": "./helper.js"
    },
    "./orm": {
      "types": "./orm.d.ts",
      "default": "./orm.js"
    },
    "./schedule": {
      "types": "./schedule.d.ts",
      "default": "./schedule.js"
    },
    "./standalone": {
      "types": "./standalone.d.ts",
      "default": "./standalone.js"
    },
    "./transaction": {
      "types": "./transaction.d.ts",
      "default": "./transaction.js"
    }
  },
  "files": [
    "*.js",
    "*.d.ts"
  ],
  "scripts": {
    "clean": "tsc -b --clean",
    "tsc:pub": "npm run clean && tsc -p ./tsconfig.json",
    "prepublishOnly": "npm run tsc:pub"
  },
  "author": "killagu <killa123@126.com>",
  "license": "MIT",
  "homepage": "https://github.com/eggjs/tegg",
  "bugs": {
    "url": "https://github.com/eggjs/tegg/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:eggjs/tegg.git",
    "directory": "core/tegg"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@eggjs/ajv-decorator": "^4.0.0-beta.1",
    "@eggjs/aop-decorator": "^4.0.0-beta.1",
    "@eggjs/controller-decorator": "^4.0.0-beta.1",
    "@eggjs/core-decorator": "^4.0.0-beta.1",
    "@eggjs/dal-decorator": "^4.0.0-beta.1",
    "@eggjs/eventbus-decorator": "^4.0.0-beta.1",
    "@eggjs/standalone-decorator": "^4.0.0-beta.1",
    "@eggjs/tegg-background-task": "^4.0.0-beta.1",
    "@eggjs/tegg-common-util": "^4.0.0-beta.1",
    "@eggjs/tegg-dynamic-inject": "^4.0.0-beta.1",
    "@eggjs/tegg-lifecycle": "^4.0.0-beta.1",
    "@eggjs/tegg-loader": "^4.0.0-beta.1",
    "@eggjs/tegg-metadata": "^4.0.0-beta.1",
    "@eggjs/tegg-orm-decorator": "^4.0.0-beta.1",
    "@eggjs/tegg-runtime": "^4.0.0-beta.1",
    "@eggjs/tegg-schedule-decorator": "^4.0.0-beta.1",
    "@eggjs/tegg-transaction-decorator": "^4.0.0-beta.1",
    "@eggjs/tegg-types": "^4.0.0-beta.1"
  },
  "devDependencies": {
    "@types/node": "22",
    "ts-node": "10",
    "typescript": "5"
  }
}
