    "@types/node": "^14.14.31",
    "cross-env": "^7.0.3",
    "eslint": "^8.0.0",
    "eslint-config-egg": "^12.0.0",
    "eslint-plugin-import": "^2.24.2",
    "espower-typescript": "^9.0.2",
    "intelli-espower-loader": "^1.0.1",
    "lerna": "^3.22.1",
    "mocha": "^7.1.0",
