{
  "name": "tegg",
  "version": "4.0.0-beta.11",
  "description": "tegg monorepo",
  "private": true,
  "type": "module",
  "homepage": "https://github.com/eggjs/tegg",
  "bugs": {
    "url": "https://github.com/eggjs/tegg/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:eggjs/tegg.git"
  },
  "engines": {
    "node": ">=22.18.0"
  },
  "packageManager": "pnpm@10.18.0",
  "scripts": {
    "lint:fix": "npm run lint -- --fix",
    "lint": "oxlint --type-aware",
    "typecheck": "npm run clean && pnpm -r run typecheck",
    "test": "vitest run",
    "test:mocha": "pnpm -r run test:mocha",
    "clean": "pnpm -r run clean",
    "build": "pnpm -r run build",
    "cov": "vitest run --coverage",
    "cov:mocha": "pnpm -r run cov:mocha",
    "ci": "npm run cov && npm run cov:mocha",
    "prepare": "husky"
  },
  "lint-staged": {
    "*": "prettier --write --ignore-unknown --cache",
    "*.{ts,js,json,md,yml}": [
      "prettier --ignore-unknown --write",
      "oxlint --type-aware --fix"
    ]
  },
  "author": "killagu <killa123@126.com>",
  "license": "MIT",
  "devDependencies": {
    "@eggjs/tsconfig": "catalog:",
    "@vitest/coverage-v8": "catalog:",
    "ajv": "catalog:",
    "husky": "catalog:",
    "js-yaml": "catalog:",
    "lint-staged": "catalog:",
    "oxlint": "catalog:",
    "oxlint-tsgolint": "catalog:",
    "prettier": "catalog:",
    "rimraf": "catalog:",
    "semver": "catalog:",
    "tsx": "catalog:",
    "typescript": "catalog:",
    "urllib": "catalog:",
    "vite": "catalog:",
    "vite-plugin-swc-transform": "catalog:",
    "vitest": "catalog:"
  }
}
