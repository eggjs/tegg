    "@eggjs/tsconfig": "catalog:",
    "@vitest/coverage-v8": "catalog:",
    "ajv": "catalog:",
    "c8": "catalog:",
    "lerna": "catalog:",
    "oxlint": "catalog:",
    "oxlint-tsgolint": "catalog:",
    "rimraf": "catalog:",
    "tsx": "catalog:",
    "typescript": "catalog:",
    "vite-plugin-swc-transform": "catalog:",
    "vitest": "catalog:",
    "vite": "catalog:"
  }
