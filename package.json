{
  "name": "tegg",
  "version": "4.0.0-beta.8",
  "description": "tegg monorepo",
  "private": true,
  "type": "module",
  "homepage": "https://github.com/eggjs/tegg",
  "bugs": {
    "url": "https://github.com/eggjs/tegg/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:eggjs/tegg.git"
  },
  "engines": {
    "node": ">=22.18.0"
  },
  "packageManager": "pnpm@10.18.0",
  "scripts": {
    "lint:fix": "npm run lint -- --fix",
    "lint": "oxlint --type-aware",
    "typecheck": "npm run clean && pnpm -r run typecheck",
    "test": "vitest run",
    "test:mocha": "pnpm -r run test:mocha",
    "clean": "pnpm -r run clean",
    "prepare-test": "pnpm -r run prepare-test",
    "build": "pnpm -r run build",
    "cov": "vitest run --coverage",
    "cov:mocha": "pnpm -r run cov:mocha",
    "ci": "npm run cov && npm run cov:mocha",
    "postci": "npm run build && npm run clean",
    "version:patch": "node scripts/version.js patch",
    "version:minor": "node scripts/version.js minor",
    "version:major": "node scripts/version.js major",
    "version:prerelease": "node scripts/version.js prerelease",
    "version:prepatch": "node scripts/version.js prepatch",
    "version:preminor": "node scripts/version.js preminor",
    "version:premajor": "node scripts/version.js premajor",
    "version:alpha": "node scripts/version.js prerelease --prerelease-tag=alpha",
    "version:beta": "node scripts/version.js prerelease --prerelease-tag=beta",
    "version:rc": "node scripts/version.js prerelease --prerelease-tag=rc"
  },
  "author": "killagu <killa123@126.com>",
  "license": "MIT",
  "devDependencies": {
    "@eggjs/tsconfig": "catalog:",
    "@vitest/coverage-v8": "catalog:",
    "ajv": "catalog:",
    "c8": "catalog:",
    "js-yaml": "catalog:",
    "oxlint": "catalog:",
    "oxlint-tsgolint": "catalog:",
    "rimraf": "catalog:",
    "semver": "catalog:",
    "tsx": "catalog:",
    "typescript": "catalog:",
    "urllib": "catalog:",
    "vite": "catalog:",
    "vite-plugin-swc-transform": "catalog:",
    "vitest": "catalog:"
  }
}
