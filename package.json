{
  "name": "tegg",
  "version": "0.0.1",
  "description": "",
  "private": true,
  "type": "module",
  "homepage": "https://github.com/eggjs/tegg",
  "bugs": {
    "url": "https://github.com/eggjs/tegg/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:eggjs/tegg.git"
  },
  "engines": {
    "node": ">=22.18.0"
  },
  "packageManager": "pnpm@10.18.0",
  "scripts": {
    "lint:fix": "npm run lint -- --fix",
    "lint": "oxlint --type-aware",
    "typecheck": "npm run clean && pnpm -r run typecheck",
    "test": "vitest run",
    "test:mocha": "pnpm -r run test:mocha",
    "pub": "lerna publish from-git",
    "pub-canary": "lerna publish --canary",
    "clean": "pnpm -r run clean",
    "prepare-test": "pnpm -r run prepare-test",
    "build": "pnpm -r run build",
    "cov": "vitest run --coverage",
    "cov:mocha": "pnpm -r run cov:mocha",
    "preci": "npm run clean && npm run lint",
    "ci": "npm run cov && npm run cov:mocha && npm run typecheck",
    "postci": "npm run build && npm run clean"
  },
  "author": "killagu <killa123@126.com>",
  "license": "MIT",
